public class Helper {

	List<Integer> ips;
	List<Byte> prefixLengths;
	List<Integer> portNumbers;
	
	public Helper() {
        ips = new ArrayList<>();
        prefixLengths = new ArrayList<>();
        portNumbers = new ArrayList<>();
	}
	
	
	public void addRoutes(int ip, byte prefixlength, int portnr) {
    	ips.add(ip);
    	prefixLengths.add(prefixlength);
    	portNumbers.add(portnr);
	}
	
	public List<Integer> selectCorrectSubset(int ip) {

	}
	
	
}
